/dts-v1/;

#include "ipq8074.dtsi"
#include "ipq8074-hk-cpu.dtsi"
#include "ipq8074-ess.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Zyxel NBG7815";
	compatible = "zyxel,nbg7815", "qcom,ipq8074";

	aliases {
		serial0 = &blsp1_uart5;
		sdhc1 = &sdhc_1;
		/* Aliases as required by u-boot to patch MAC addresses */
		ethernet0 = &dp1;
		ethernet1 = &dp2;
		ethernet2 = &dp3;
		ethernet3 = &dp4;
		ethernet4 = &dp5;
		ethernet5 = &dp6;
		label-mac-device = &dp1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "console=ttyMSM0,115200,n8 root=/dev/ram0 rw init=/init";
		bootargs-append = " swiotlb=1";
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset_to_default";
			linux,code = <KEY_RESTART>;
			gpios = <&tlmm 54 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			debounce-interval = <60>;
		};
	};
};

&tlmm {
	mdio_pins: mdio-pins {
		mdc {
			pins = "gpio68";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mdio {
			pins = "gpio69";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};
	uart_pins: uart_pins {
		mux {
			pins = "gpio23", "gpio24";
			function = "blsp4_uart1";
			drive-strength = <8>;
			bias-disable;
		};
	};

	/* add 2nd i2c bus for temperature sensor CLK & DATA - needs patch in ipq8074.dts
	i2c_1_pins: i2c_1_pinmux {
		mux {
			pins = "gpio50", "gpio51";
			function = "blsp3_i2c";
			drive-strength = <8>;
			bias-disable;
		};
	};*/

};

&blsp1_uart5 {
	status = "okay";
};

&prng {
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&crypto {
	status = "okay";
};

&qpic_bam {
	status = "okay";
};

&mdio {
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	phy-reset-gpio = <&tlmm 37 0 &tlmm 31 1>;
	qca8075_1: ethernet-phy@0 {
		reg = <0>;
	};
	qca8075_2: ethernet-phy@1 {
		reg = <1>;
	};
	qca8075_3: ethernet-phy@2 {
		reg = <2>;
	};
	qca8075_4: ethernet-phy@3 {
		reg = <3>;
	};
	qca8081: ethernet-phy@4 {
		reg = <28>;
	};
	aqr107: ethernet-phy@5 {
		compatible ="ethernet-phy-ieee802.3-c45";
		reg = <8>;
	};
};

