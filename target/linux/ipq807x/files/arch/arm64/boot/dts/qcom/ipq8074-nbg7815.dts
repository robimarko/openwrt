&soc {
	dp1: dp1 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <1>;
		reg = <0x3a001000 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_1>;
		phy-mode = "sgmii";
	};

	dp2: dp2 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <2>;
		reg = <0x3a001200 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_2>;
		phy-mode = "sgmii";
		};

	dp3: dp3 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <3>;
		reg = <0x3a001400 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_3>;
		phy-mode = "sgmii";
	};

	dp4: dp4 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <4>;
		reg = <0x3a001600 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8075_4>;
		phy-mode = "sgmii";
	};

	dp5: dp5 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <5>;
		reg = <0x3a001800 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&qca8081>;
		phy-mode = "sgmii";
	};

	dp6: dp6 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <6>;
		reg = <0x3a007000 0x3fff>;
		qcom,mactype = <1>;
		local-mac-address = [00 00 00 00 00 00];
		phy-handle = <&aqr107>;
		phy-mode = "sgmii";
	};
};

&blsp1_i2c2 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-names = "default";
	status = "ok";

	lp5569@32 {
		compatible = "ti,lp5569";
		reg = <0x32>;
		clock-mode = /bits/8 <1>;

		chan0 {
			chan-name = "D1";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan1 {
			chan-name = "D2";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan2 {
			chan-name = "D3";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan3 {
			chan-name = "D4";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan4 {
			chan-name = "D5";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan5 {
			chan-name = "D6";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan6 {
			chan-name = "D7";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan7 {
			chan-name = "D8";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan8 {
			chan-name = "D9";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
	};

	lp5569@35 {
		compatible = "ti,lp5569";
		reg = <0x35>;
		clock-mode = /bits/8 <2>;

		chan0 {
			chan-name = "D10";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan1 {
			chan-name = "D11";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan2 {
			chan-name = "D12";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan3 {
			chan-name = "D13";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan4 {
			chan-name = "D14";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan5 {
			chan-name = "D15";
			led-cur = /bits/ 8 <0xDF>;
