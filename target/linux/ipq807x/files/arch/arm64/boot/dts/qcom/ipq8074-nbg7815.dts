		};
		chan7 {
			chan-name = "D17";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
		chan8 {
			chan-name = "D18";
			led-cur = /bits/ 8 <0xDF>;
			max-cur = /bits/ 8 <0xFF>;
		};
	};

	tmp103@70 {
		compatible = "ti,tmp103";
		reg = <0x70>;
	};
};

&sdhc_1 {
	qcom,clk-rates = <400000 25000000 50000000 100000000 192000000 384000000>;
	qcom,bus-speed-mode = "HS400_1p8v", "HS200_1p8v", "DDR_1p8v";
	qcom,nonremovable;
	status = "ok";
};

&wifi {
	status = "okay";

	qcom,ath11k-calibration-variant = "Zyxel-NBG7815";
};
