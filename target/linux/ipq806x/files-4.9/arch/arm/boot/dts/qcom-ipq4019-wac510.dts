#include "qcom-ipq4019.dtsi"
#include "qcom-ipq4019-bus.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "Netgear WAC510";
	compatible = "netgear,wac510", "qcom,ipq4019";

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		rsvd1@87000000 {
			/* Reserved for other subsystem */
			reg = <0x87000000 0x500000>;
			no-map;
		};
		wifi_dump@87500000 {
			reg = <0x87500000 0x600000>;
			no-map;
		};
		rsvd2@87B00000 {
			/* Reserved for other subsystem */
			reg = <0x87B00000 0x500000>;
			no-map;
		};
	};

	soc {
		tcsr@194b000 {
			/* select hostmode */
			compatible = "qcom,tcsr";
			reg = <0x194b000 0x100>;
			qcom,usb-hsphy-mode-select = <TCSR_USB_HSPHY_HOST_MODE>;
			status = "ok";
		};

		ess_tcsr@1953000 {
			compatible = "qcom,tcsr";
			reg = <0x1953000 0x1000>;
			qcom,ess-interface-select = <TCSR_ESS_PSGMII>;
		};

		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};

		pinctrl@1000000 {
			serial_pins: serial_pinmux {
				mux {
					pins = "gpio60", "gpio61";
					function = "blsp_uart0";
					bias-disable;
				};
			};

			spi_0_pins: spi_0_pinmux {
				pinmux {
					function = "blsp_spi0";
					pins = "gpio55", "gpio56", "gpio57";
				};
				pinmux_cs {
					function = "gpio";
					pins = "gpio54", "gpio59";
				};
				pinconf {
					pins = "gpio55", "gpio56", "gpio57";
					drive-strength = <12>;
					bias-disable;
				};
				pinconf_cs {
					pins = "gpio54", "gpio59";
					drive-strength = <2>;
					bias-disable;
					output-high;
				};
			};
		};

		blsp_dma: dma@7884000 {
			status = "ok";
		};

		spi_0: spi@78b5000 {
			pinctrl-0 = <&spi_0_pins>;
			pinctrl-names = "default";
			cs-gpios = <&tlmm 54 0>, <&tlmm 59 0>;
			status = "ok";

			mx25l25635e@0 {
				status = "disabled";
			};

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0>;
				compatible = "mx25l1606e", "n25q128a11";
				linux,modalias = "m25p80", "mx25l1606e", "n25q128a11";
				spi-max-frequency = <24000000>;

				partitions {
					compatible = "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;

					partition0@0 {
						label = "0:SBL1";
						reg = <0x00000000 0x00040000>;
						read-only;
					};
					partition1@40000 {
						label = "0:MIBIB";
						reg = <0x00040000 0x00020000>;
						read-only;
					};
					partition2@60000 {
						label = "0:QSEE";
						reg = <0x00060000 0x00060000>;
						read-only;
					};
					partition3@c0000 {
						label = "0:CDT";
						reg = <0x000c0000 0x00010000>;
						read-only;
					};
					partition4@d0000 {
						label = "0:DDRPARAMS";
						reg = <0x000d0000 0x00010000>;
						read-only;
					};
					partition5@e0000 {
						label = "0:APPSBLENV"; /* uboot env*/
						reg = <0x000e0000 0x00010000>;
						read-only;
					};
					partition5@f0000 {
						label = "0:APPSBL"; /* uboot */
						reg = <0x000f0000 0x00080000>;
						read-only;
					};
					partition5@1e0000 {
						label = "0:MANUDATA";
						reg = <0x001e0000 0x00010000>;
						read-only;
					};
					partition5@1f0000 {
						label = "0:ART";
						reg = <0x001f0000 0x00010000>;
						read-only;
					};
				};
			};

			mt29f@1 {
				status = "ok";
				#address-cells = <1>;
				#size-cells = <0>;
				compatible = "spinand,mt29f", "w25n01gv";
				reg = <1>;
				spi-max-frequency = <24000000>;
				partitions {
					compatible = "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;

					partition0@0 {
						label = "ubi";
						reg = <0x00000000 0x07000000>;
					};
					partition1@7000000 {
						label = "var_config";
						reg = <0x07000000 0x00f00000>;
						read-only;
					};
					partition2@7f00000 {
						label = "Oops_log";
						reg = <0x07f00000 0x000c0000>;
						read-only;
					};
				};
			};
		};

		led_spi {
			compatible = "spi-gpio";
			#address-cells = <1>;
			ranges;

			gpio-sck = <&tlmm 5 GPIO_ACTIVE_HIGH>;
			gpio-mosi = <&tlmm 4 GPIO_ACTIVE_HIGH>;
			num-chipselects = <0>;

			led_gpio: led_gpio@0 {
				compatible = "fairchild,74hc595";
				reg = <0>;
				gpio-controller;
				#gpio-cells = <2>;
				registers-number = <1>;
				spi-max-frequency = <1000000>;
			};
		};

		gpio-leds {
			compatible = "gpio-leds";

			power_amber: power_amber {
				label = "wac510:amber:power";
				gpios = <&led_gpio 6 GPIO_ACTIVE_LOW>;
			};

			power_green: power_green {
				label = "wac510:green:power";
				gpios = <&led_gpio 5 GPIO_ACTIVE_LOW>;
			};

			wlan2g_blue {
				label = "wac510:blue:wlan2g";
				gpios = <&led_gpio 4 GPIO_ACTIVE_LOW>;
			};

			wlan2g_green {
				label = "wac510:green:wlan2g";
				gpios = <&led_gpio 3 GPIO_ACTIVE_LOW>;
			};

			wlan5g_blue {
				label = "wac510:blue:wlan5g";
				gpios = <&led_gpio 2 GPIO_ACTIVE_LOW>;
			};

			wlan5g_green {
				label = "wac510:green:wlan5g";
				gpios = <&led_gpio 1 GPIO_ACTIVE_LOW>;
			};

			act_green {
				label = "wac510:green:act";
				gpios = <&led_gpio 0 GPIO_ACTIVE_LOW>;
			};
		};

		gpio-keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 63 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			};
		};

		serial@78af000 {
			pinctrl-0 = <&serial_pins>;
			pinctrl-names = "default";
			status = "ok";
		};

		cryptobam: dma@8e04000 {
			status = "ok";
		};

		crypto@8e3a000 {
			status = "ok";
		};

		watchdog@b017000 {
			status = "ok";
		};

		usb3_ss_phy: ssphy@9a000 {
			status = "ok";
		};

		usb3_hs_phy: hsphy@a6000 {
			status = "ok";
		};

		usb3: usb3@8af8800 {
			status = "ok";
		};

		usb2_hs_phy: hsphy@a8000 {
			status = "ok";
		};

		usb2: usb2@60f8800 {
			status = "ok";
		};

		mdio@90000 {
			status = "okay";
		};

		ess-switch@c000000 {
			status = "okay";
		};

		ess-psgmii@98000 {
			status = "okay";
		};

		edma@c080000 {
			status = "okay";
		};

		wifi@a000000 {
			status = "okay";
			qcom,ath10k-calibration-variant = "Netgear-WAC510";
		};

		wifi@a800000 {
			status = "okay";
			qcom,ath10k-calibration-variant = "Netgear-WAC510";
		};
	};
};
